<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="YNAET" namespace="YNAET.Entities">
  <class name="Expense" table="Expenses" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="id" column="id" type="long">
      <generator class="native" />
    </id>

    <property name="amount"  />
    <property name="payee" />
    <property name="category" />
    <property name="account" />
    <property name="date" />
    <property name="repeat" />
    <property name="memo" />
    <property name="impulse" />
    <property name="colorCode" />
  </class>
</hibernate-mapping>